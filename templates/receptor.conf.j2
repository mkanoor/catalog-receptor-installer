[default]
node_id={{ lookup('file', receptor_config_dir+'/rh_ansible_'+tower_account_id+'/uuid') }}
debug=False

[plugin_receptor_catalog]
token={{ tower_token }}
username={{ tower_user }}
password={{ tower_password }}
url={{ tower_url }}
verify_ssl={{ tower_validate_certs }}

[node]
peers=wss://{{ c_rh_c_host }}/wss/receptor-controller/gateway
server_disable=true
ws_heartbeat=1800

[auth]
client_cert=/etc/pki/consumer/cert.pem
client_key=/etc/pki/consumer/key.pem
